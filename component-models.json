        "component": "text",
        "valueType": "string",
        "label": "Background Video",
        "name": "BackgroundVideo",
        "description": "Upload video for the Primary Hero",
        "required": true,
        "condition": {
          "===": [{ "var": "PrimarybackgroundType" }, "videoSlide"]
        }
      },
      {
        "component": "boolean",
        "label": "Enable Overlay?",
        "name": "EnableOverlay",
        "valueType": "boolean",
        "description": "Enable Overlay video for the Primary Hero"
      },
      {
        "component": "text-input",
        "valueType": "string",
        "name": "overlayTitle",
        "label": "Overlay Title",
        "required": true,
        "condition": {
          "===": [{ "var": "EnableOverlay" }, true]
        }
      },

      {
        "component": "reference",
        "valueType": "string",
        "name": "icon",
        "label": "Icon",
        "description": "Upload the icon image",
        "condition": {
          "===": [{ "var": "PrimarybackgroundType" }, "imageSlide"]
        }
      },
      {
        "component": "text",
        "name": "imageAlt",
        "label": "Icon Alt text",
        "valueType": "string",
        "description": "Pick alt text from AEM DAM with ability to override it",
        "condition": {
          "===": [{ "var": "PrimarybackgroundType" }, "imageSlide"]
        }
      },
      {
        "component": "select",
        "name": "textcolor",
        "label": "Text Color",
        "description": "Select the Text Color",
        "value": "white",
        "options": [
          { "name": "Black", "value": "black" },
          { "name": "White", "value": "white" }
        ],
        "condition": {
          "===": [{ "var": "PrimarybackgroundType" }, "imageSlide"]
        }
      },
      {
        "component": "text",
        "name": "ctalabel",
        "label": "CTA Label",
        "valueType": "string",
        "description": "Enter label to display on the CTA for each card",
        "condition": {
          "===": [{ "var": "PrimarybackgroundType" }, "imageSlide"]
        }
      },
      {
        "component": "aem-content",
        "name": "ctaLink",
        "label": "CTA Link",
        "description": "Enter the URL to navigate when CTA is clicked for each card",
        "validation": {
          "rootPath": "/content/lilly-com-eds"
        },
        "condition": {
          "===": [{ "var": "PrimarybackgroundType" }, "imageSlide"]
        }
      },
      {
        "component": "select",
        "name": "targetPath",
        "label": "Target Path",
        "description": "Select the target path for the authored CTA URL",
        "value": "sameTab",
        "options": [
          { "name": "Same Tab", "value": "sameTab" },
          { "name": "New Tab", "value": "newTab" }
        ],
        "condition": {
          "===": [{ "var": "PrimarybackgroundType" }, "imageSlide"]
        }
      },
      {
        "component": "checkbox-group",
        "label": "ExitInterstitial",
        "name": "exitInterstitial",
        "valueType": "string",
        "options": [{ "name": "ExitInterstitial", "value": "true" }],
        "condition": {
          "===": [{ "var": "PrimarybackgroundType" }, "imageSlide"]
        }
      },
      {
        "component": "text",
        "name": "text",
        "label": "Title",
        "required": true,
        "valueType": "string",
        "description": "Enter main heading for the Primary Hero",
        "validation": {
          "minLength": 0,
          "maxLength": 70,
          "regExp": "^foo:.*",
          "customErrorMsg": "Maximum Characters length limit should not be more than 70"
        }
      },
      {
        "component": "richtext",
        "name": "description",
        "label": "Description",
        "valueType": "string",
        "description": "Enter description for additional context"
      }
    ]
